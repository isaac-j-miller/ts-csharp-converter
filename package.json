{
  "name": "@costar/ts-csharp-converter",
  "version": "0.0.10",
  "description": "Convert typescript to C# data classes",
  "main": "./index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "test": "node node_modules/jest/bin/jest.js",
    "test-inspect": "node --inspect  node_modules/jest/bin/jest.js --no-cache",
    "run-test": "node -r ts-node/register -r tsconfig-paths/register ./src/run-test.ts",
    "run-test-inspect": "node -r ts-node/register -r tsconfig-paths/register --inspect ./src/run-test.ts",
    "run-test-rw": "node -r ts-node/register -r tsconfig-paths/register ./src/run-test-rw.ts",
    "run-test-rw-inspect": "node -r ts-node/register -r tsconfig-paths/register --inspect ./src/run-test-rw.ts",
    "prettier": "prettier --write .",
    "prettier-check": "prettier .",
    "check-ts": "tsc --noEmit",
    "lint": "./node_modules/.bin/eslint ./src/**/*.ts",
    "generate-types": "rm -rf ./dist/types && tsc --declarationDir ./dist/types --outDir ./tmp/dist && rm -rf ./tmp/dist",
    "build": "node -r ts-node/register -r tsconfig-paths/register ./esbuild.config.ts && pnpm generate-types && cp ./package.json ./dist/package.json && mkdir -p ./dist/bin && cp ./bin/cli ./dist/bin/cli && cp .npmrc ./dist/.npmrc && cp -a ./resources/ ./dist/resources/",
    "check-all": "pnpm prettier-check && pnpm check-ts && pnpm lint && pnpm test"
  },
  "author": "Isaac Miller",
  "license": "BSD-3-Clause",
  "dependencies": {
    "argparse": "^2.0.1",
    "deepmerge": "^4.2.2",
    "ejs": "^3.1.8",
    "glob": "^8.0.3",
    "minimatch": "^5.1.0",
    "ts-morph": "^15.1.0",
    "type-fest": "^2.18.0",
    "typescript": "^4.7.4",
    "uuid": "^7"
  },
  "bin": "./bin/cli",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/plugin-transform-typescript": "^7.18.12",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@types/argparse": "^2.0.10",
    "@types/ejs": "^3.1.1",
    "@types/glob": "^7.2.0",
    "@types/jest": "^28.1.6",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^18.6.1",
    "@types/uuid": "^7",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "esbuild": "^0.14.53",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0"
  }
}
